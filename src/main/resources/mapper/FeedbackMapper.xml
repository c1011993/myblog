<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.javatiaocao.myblog.mapper.FeedbackMapper">

    <select id="getAllFeedback" resultType="com.javatiaocao.myblog.model.Feedback">
        SELECT * FROM feedback ORDER BY id desc
    </select>
    <select id="getAllPrivateWord" resultType="com.javatiaocao.myblog.model.PrivateWord">
        select * from privateword order by publisherDate desc
    </select>
<!--    <select id="getPublisherIdList" resultType="java.lang.String">
        SELECT DISTINCT publisherId FROM privateword
    </select>-->
</mapper>